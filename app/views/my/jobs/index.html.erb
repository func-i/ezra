<% provide :page_name, "My Jobs" %>
<% provide :page_desc, "List of Jobs you have created" %>
<% provide :title, "Jobs" %>

<div class="row">

  <div class="span3">
    <%= render 'shared/dashboard_nav' %>
  </div>

  <div class="span9">
    <% @jobs.each do |job| %>
      <%= render 'shared/job', :job => job %>
    <% end %>
  </div>

</div>